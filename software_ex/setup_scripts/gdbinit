set debuginfod enabled off

set $load_ext = 1

define init-gef
	set $load_ext = 0
	source ~/.gef-2025.01.py
end
document init-gef
	Initializes GEF (GDB Enhanced Features)
end

define init-pwn
	set $load_ext = 0
	source /home/ubuntu/tools/gdb_ext/pwndbg/gdbinit.py
end
document init-pwn
Initializes PwnDBG
end
